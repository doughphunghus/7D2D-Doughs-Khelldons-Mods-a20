<!--
Khelldon - Let's find all the blocks made of wood and add some nails and bent nails to harvest
Also let's make sure you need nails to repair them as well.
Updated: Doors included
-->
<append xpath="/blocks/block
[
@name='woodWeakNoUpgradeMaster'
or @name='woodNoUpgradeMaster'
or @name='frameShapes'
or @name='woodShapes'
or @name='rWoodMaster'
or @name='whitePicketFenceSheet'
or @name='cntCabinetBottom'
or @name='cntCupboardCabinetRedTopClosed'
or @name='cntWoodDesk01Closed'
or @name='cntSecureStorageChest'
or @name='endTable'
or @name='ladderWood'
or @name='window03Wood'
or @name='secureDoorWooden'
or @name='houseFrontDoor1_v1'
or @name='houseFrontDoor2_v1'
or @name='cellarDoorDoubleWood'
or @name='shuttersWood01'
or @name='shuttersWood02'
]">
	<drop event="Harvest" name="resourceNail" count="1" tag="nailsHarvest"/> <!-- DOUGH: Custom tag-->
	<drop event="Harvest" name="resourceNailBent" count="1,2" tag="allHarvest"/> 
</append>


<append xpath="/blocks/block
[
@name='frameShapes'
]
/property
[
@class='RepairItems'
]">
<property name="resourceNail" value="2"/>
</append>


<append xpath="/blocks/block
[
@name='woodWeakNoUpgradeMaster'
or @name='woodShapes'
or @name='woodNoUpgradeMaster'
or @name='rWoodMaster'
or @name='cntCabinetBottom'
or @name='cntCupboardCabinetRedTopClosed'
or @name='cntSecureStorageChest'
or @name='window03Wood'
]
/property
[
@class='RepairItems'
]">
<property name="resourceNail" value="4"/>
</append>


<append xpath="/blocks/block
[
@name='endTable'
or @name='ladderWood'
or @name='whitePicketFenceSheet'
or @name='cntWoodDesk01Closed'
]
/property
[
@class='RepairItems'
]">
<property name="resourceNail" value="8"/>
</append>


<append xpath="/blocks/block
[
@name='secureDoorWooden'
or @name='houseFrontDoor1_v1'
or @name='houseFrontDoor2_v1'
or @name='cellarDoorDoubleWood'
]
/property
[
@class='RepairItems'
]">
<property name="resourceNail" value="12"/>
</append>




